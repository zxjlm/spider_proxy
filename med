import re
from bs4 import BeautifulSoup
import requests

url_raw = 'http://www.a-hospital.com/w/'
url = url_raw+"玉米须"
req = requests.get(url)
soup1=BeautifulSoup(req.content,'lxml')

img_url=soup1.find(class_="image")
print(img_url.img["src"])
